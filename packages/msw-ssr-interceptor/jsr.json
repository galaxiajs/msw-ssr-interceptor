{
	"name": "msw-ssr-interceptor",
	"version": "0.0.0",
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "deno.ns"]
	},
	"unstable": ["sloppy-imports"],
	"exports": {
		".": "./src/index.ts",
		"./node": "./src/node.ts",
		"./playwright": "./src/playwright.ts"
	},
	"publish": {
		"include": [
			"jsr.json",
			"../../LICENSE",
			"../../README.md",
			"../../CODE_OF_CONDUCT.md",
			"../../CONTRIBUTING.md",
			"src/**/*.ts"
		],
		"exclude": ["src/**/*.test.ts", "src/**/*.spec.ts"]
	}
}
